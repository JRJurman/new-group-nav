{
  "name": "New Group Nav",
  "description": "Get details and write notes for your tab groups",
  "version": "2.0.0",
  "manifest_version": 3,
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "icons": {
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png",
    "512": "icon512.png"
  },
  "permissions": ["tabs", "tabGroups", "storage"]
}
